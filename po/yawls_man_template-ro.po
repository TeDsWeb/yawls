#. type: Plain text
#: man/yawls.1:53
msgid "B<-r> (B<--restore>)"
msgstr "B<-r> (B<--restore>)"

#. type: Plain text
#: man/yawls.1:55
msgid "Resets all config files:"
msgstr "Resetează toate fişiere de configurare:"


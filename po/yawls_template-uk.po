msgid "_Help"
msgstr "_Довідка"

#: src/com/blogspot/thedsweb/gui/Show.java:377
msgid "_Preferences"
msgstr "_Налаштування"

#: src/com/blogspot/thedsweb/gui/Show.java:382
msgid "_About"
msgstr "_Про програму"

#: src/com/blogspot/thedsweb/gui/Show.java:384
msgid "FAQ"
msgstr "ЧаПи"


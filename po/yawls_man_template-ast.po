#. type: Plain text
#: man/yawls.1:9
msgid ""
"Yawls (Yet Another Webcam Light Sensor) adjusts the brightness level of your "
"display by using the internal/externel webcam of your notebook as an ambient "
"light sensor."
msgstr ""

#. type: Plain text
#: man/yawls.1:11
msgid "Yawls obtains configuration data from the following sources:"
msgstr "Yawls saca los datos pa la configuracion de les fontes siguientes:"

#. type: Plain text
#: man/yawls.1:13 man/yawls.1:57
msgid "/etc/yawls.properties"
msgstr "/etc/yawls.properties"

#. type: Plain text
#: man/yawls.1:15
msgid "/etc/cron.d/yawls"
msgstr "/etc/cron.d/yawls"

#. type: Plain text
#: man/yawls.1:17 man/yawls.1:59
msgid "/var/lib/yawls/*"
msgstr "/var/lib/yawls/*"

#. type: SS
#: man/yawls.1:18
#, no-wrap
msgid "Option Summary:"
msgstr "Resume d'opciones:"

#. type: TP
#: man/yawls.1:19
#, no-wrap
msgid "B<--cron>\t\tor B<-c>\tuse webcam as brightness reference (one time)"
msgstr "B<--cron>\t\to B<-c>\tusa la webcam como referencia de brillu (una vegada)"

#. type: TP
#: man/yawls.1:21
#, no-wrap
msgid "B<--daemon>\t\tor B<-d>\tuse webcam as brightness reference (continuously)"
msgstr "B<--daemon>\t\to B<-d>\tusa la webcam como referencia de brillu (continuamente)"

#. type: TP
#: man/yawls.1:23
#, no-wrap
msgid "B<--gui>\t\tor B<-g>\tstarts gui"
msgstr "B<--gui>\t\to B<-g>\tprincipia la interfaz gráfica"

#. type: TP
#: man/yawls.1:25
#, no-wrap
